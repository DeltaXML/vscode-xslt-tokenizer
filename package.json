{
	"name": "semantic-tokens-sample",
	"displayName": "semantic-tokens-sample",
	"description": "Sample showing the Semantic Tokens Provider API",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:XPath"
	],
	"enableProposedApi": true,
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "XPath",
				"extensions": [
					".xpath"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"test": "jest"
	},
	"devDependencies": {
		"@types/jest": "^24.0.25",
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.32.0",
		"jest": "^24.9.0",
		"ts-jest": "^24.3.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
